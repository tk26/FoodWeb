<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.IUB_P565_Group1.FoodWeb" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.IUB_P565_Group1.FoodWeb.MainMenu"
            android:label="FoodWeb"
            android:screenOrientation="landscape" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.IUB_P565_Group1.FoodWeb.Game"
            android:label="Game"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name="com.IUB_P565_Group1.FoodWeb.OptionsMenu"
            android:label="@string/title_activity_options_menu"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name="com.IUB_P565_Group1.FoodWeb.HighScore"
            android:label="@string/title_activity_high_score"
            android:screenOrientation="landscape">
        </activity>
        <activity
            android:name="com.IUB_P565_Group1.FoodWeb.Help"
            android:label="@string/title_activity_help"
            android:screenOrientation="landscape">
        </activity>
    </application>


</manifest>
